    "require": {
        "php": ">=7.2",
        "monolog/monolog": "^1",
        "vlucas/phpdotenv": "^2",
        "slim/slim": "^3",
        "slim/twig-view": "^2",
        "slim/flash": "^0",
        "bryanjhv/slim-session": "^3",
